          {{item.description}}
          ${{item.price}}</li>
        {{/each}}
        </ul>
      </div>
      <div class="col-xs-4 col-sm-6">
        <h3>New Added Attractions</h3>
        <ul>
          {{#each newAttractions as |item|}}
          <li>{{item.name}}<br>
            {{item.description}}
            ${{item.price}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>

  <div class="col-sm-9 col-md-offset-3">
    Level 1: .col-sm-9
    <div class="row">
      <div class="col-xs-8 col-sm-6">
        <h3>Soups</h3>
        <ul>
        {{#each soups as |item|}}
        <li>{{item.name}}<br>
          {{item.description}}
          ${{item.price}}</li>
        {{/each}}
        </ul>
      </div>
      <div class="col-xs-4 col-sm-6">
        <h3>Salads</h3>
        <ul>
        {{#each salads as |item|}}
        <li>{{item.name}}<br>
          {{item.description}}
          ${{item.price}}</li>
        {{/each}}
        </ul>
      </div>
    </div>
  </div>

  <div class="col-sm-9 col-md-offset-3">
    <div class="row">
      <div class="col-xs-8 col-sm-6">
        <h3>Beverages</h3>
        <ul>
        {{#each dayBeverages as |item|}}
        <li>{{item.name}}<br>
          {{item.description}}
          ${{item.price}}</li>
        {{/each}}
        </ul>
      </div>
      <div class="col-xs-4 col-sm-6">

      </div>
    </div>
  </div>
</div>




