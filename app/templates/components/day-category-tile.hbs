        {{/each}}
        </ul>
      </div>
      <div class="col-xs-4 col-sm-6">
        <h3>Side Orders</h3>
        <ul>
        {{#each sideOrders as |item|}}
        <li>{{item.name}}<br>
          {{item.description}}
          ${{item.price}}</li>
        {{/each}}
        </ul>
      </div>
    </div>
  </div>

  <div class="col-sm-9 col-md-offset-3">
    Level 1: .col-sm-9
    <div class="row">
      <div class="col-xs-8 col-sm-6">
